{
  "name": "raw-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "flow": "flow --quiet",
    "eslint": "eslint --cache --cache-location 'misc/.eslintcache' src/",
    "prettier": "prettier --config .prettierrc --write 'src/**/*.js'"
  },
  "devDependencies": {
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.45.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.66.0",
    "prettier": "^1.10.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/js/**/*.{js}"
    ],
    "coverageReporters": [
      "lcov",
      "cobertura"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  }
}
